(defun custom/strip-region-prefix (prefix)
  (interactive)
  (clipboard-kill-ring-save) (custom/strip-prefix
                              prefix
                              (buffer-substring (region-beginning) (region-end)))
  (with-temp-buffer
    (yank)
    (insert-for-yank-1
     (custom/strip-prefix prefix
                          (buffer-substring
                           (point-min) (point-max))))
    (clipboard-kill-region (point-min) (point-max))))
